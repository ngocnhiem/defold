    map { input: GAMEPAD_START type: GAMEPAD_TYPE_BUTTON index: 9
    }
    map { input: GAMEPAD_RPAD_LEFT type: GAMEPAD_TYPE_BUTTON index: 0
    }
    map { input: GAMEPAD_RPAD_UP type: GAMEPAD_TYPE_BUTTON index: 3
    }
}
